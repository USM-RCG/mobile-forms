	    <bind nodeset="/data/individualPointOfContactPhoneNumber" type="int" required="string-length(string(/data/individualPhoneNumber)) = 0 and (not(selected(/data/individualAgeUnits, 'Months')) and not(/data/individualAge &lt; 12))"
jr:requiredMsg="hueruberbl"
 constraint="(regex(., '^[0-9]{9}$'))"
