            <value>Was your house sprayed in the past 12 months?</value>
          </text>
          <text id="/data/houseSprayed:option0">
            <value>No</value>
          </text>filledFormRoot
          <text id="/data/houseSprayed:option1">
            <value>Yes</value>
          </text>
          <text id="/data/houseSprayed:option2">
            <value>Don't Know / Not Sure</value>
          </text>

          <text id="/data/reasonWhyNotSprayed:label">
            <value>Why was your house not sprayed in the past 12 months?</value>
          </text>
          <text id="/data/reasonWhyNotSprayed:option0">
            <value>Not contacted</value>
          </text>
          <text id="/data/reasonWhyNotSprayed:option1">
            <value>Nobody came back to spray my house</value>
          </text>
          <text id="/data/reasonWhyNotSprayed:option2">
            <value>Don't Know / Not Sure</value>
          </text>
          <text id="/data/reasonWhyNotSprayed:option3">
            <value>Didn't want house sprayed</value>
          </text>

          <text id="/data/whyNotSprayed01:label">
            <value>Causes ill health effects</value>
          </text>
	  <text id="/data/whyNotSprayed02:label">
            <value>Dangerous for animals</value>
          </text>
          <text id="/data/whyNotSprayed03:label">
            <value>Disruptive, annoying or disagreeable</value>
          </text>
          <text id="/data/whyNotSprayed04:label">
            <value>Attitude of sprayers</value>
          </text>
          <text id="/data/whyNotSprayed05:label">
            <value>Damages furniture or walls</value>
          </text>
          <text id="/data/whyNotSprayed06:label">
            <value>Not effective</value>
          </text>
          <text id="/data/whyNotSprayed07:label">
            <value>Not applicable to us</value>
          </text>
          <text id="/data/whyNotSprayed08:label">
            <value>Use bed nets instead</value>
          </text>
          <text id="/data/whyNotSprayed09:label">
            <value>Don't have time</value>
          </text>
          <text id="/data/whyNotSprayed10:label">
            <value>Time is inconvenient</value>
          </text>
          <text id="/data/whyNotSprayed11:label">
            <value>Other</value>
          </text>
          <text id="/data/whyNotSprayed12:label">
            <value>Don't know / not sure</value>
          </text>
  

        </translation>
      </itext>

	    <bind nodeset="/data/processedByMirth" type="boolean" readonly="true()" />
	    <bind nodeset="/data/needsReview" type="string" readonly="true()" />
	    <bind nodeset="/data/fieldWorkerExtId" type="string" readonly="true()" />
	    <bind nodeset="/data/distributionDateTime" type="datetime" readonly="true()" />
            <bind nodeset="/data/locationExtId" type="string" readonly="true()"/>

      <bind nodeset="/data/beds" type="int" required="true()"/>
      <bind nodeset="/data/netsCurrent" type="int" required="true()"/>
      <bind nodeset="/data/ITNsCurrent" type="int" required="true()" relevant="/data/netsCurrent > 0"/>
      <bind nodeset="/data/ITNGoodN" type="int" required="true()" relevant="/data/ITNsCurrent > 0"/>
      <bind nodeset="/data/netsNeeded" type="int" required="true()" calculate="if((string(/data/ITNGoodN) = ''), /data/beds, (/data/beds - /data/ITNGoodN))"/>
      <bind nodeset="/data/netsSupplied/netCode" type="string" required="true()"/>
      <bind nodeset="/data/netsSupplied/netHung" type="select1" required="true()"/>
      <bind nodeset="/data/walls" type="select1" required="true()" />
      <bind nodeset="/data/wallGaps" type="select1" required="true()" />
      <bind nodeset="/data/roof" type="select1" required="true()" />
      <bind nodeset="/data/roofGaps" type="select1" required="true()" />
      <bind nodeset="/data/eaves" type="select1" required="true()" />
      <bind nodeset="/data/patio" type="select1" required="true()" />
      <bind nodeset="/data/doors" type="select1" required="true()" />
      <bind nodeset="/data/windows" type="select1" required="true()" />
      <bind nodeset="/data/aircondition" type="select1" required="true()" />
      <bind nodeset="/data/electricity" type="select1" required="true()" />
      <bind nodeset="/data/houseSprayed" type="select1" required="true()" />
      <bind nodeset="/data/reasonWhyNotSprayed" type="select1" required="selected(/data/houseSprayed, '0')" relevant="selected(/data/houseSprayed, '0')" />
      <bind nodeset="/data/whyNotSprayed01" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed02" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed03" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed04" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed05" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed06" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed07" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed08" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed09" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed10" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed11" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />
      <bind nodeset="/data/whyNotSprayed12" type="select1" required="selected(/data/reasonWhyNotSprayed, '4')" relevant="selected(/data/reasonWhyNotSprayed, '4')" />

    </model>
  </h:head>
