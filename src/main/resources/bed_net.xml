          </text>
          <text id="/data/wallGaps:option0">
            <value>No</value>
          </text>
          <text id="/data/wallGaps:option1">
            <value>Yes (a few)</value>
          </text>
          <text id="/data/wallGaps:option2">
            <value>Yes (many)</value>
          </text>

