          </text>
          <text id="/data/electricity:option0">
            <value>None</value>
          </text>
          <text id="/data/electricity:option1">
            <value>"L1" metered connection</value>
          </text>
          <text id="/data/electricity:option2">
            <value>"L2" metered connection</value>
          </text>
          <text id="/data/electricity:option3">
            <value>Paid connection to neighbor's L1 or L2 source</value>
          </text>
          <text id="/data/electricity:option4">
            <value>Other free public electrical connection</value>
          </text>
          <text id="/data/electricity:option5">
            <value>Private generator</value>
          </text>
          <text id="/data/electricity:option6">
            <value>Solar panels</value>
          </text>
          <text id="/data/electricity:option7">
            <value>Gas or kerosene lamps</value>
          </text>
          <text id="/data/electricity:option8">
            <value>Candles</value>
          </text>
          <text id="/data/electricity:option9">
            <value>Wood fire</value>
          </text>
          <text id="/data/electricity:option10">
            <value>Don't know / not sure</value>
          </text>
          <text id="/data/electricity:option11">
            <value>Other</value>
          </text>

