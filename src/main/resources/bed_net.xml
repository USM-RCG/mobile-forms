          <text id="/data/walls:option0">
            <value>Brick</value>
          </text>
          <text id="/data/walls:option1">
            <value>Wood</value>
          </text>
          <text id="/data/walls:option2">
            <value>Cement</value>
          </text>
          <text id="/data/walls:option3">
            <value>Mud</value>
          </text>
          <text id="/data/walls:option4">
            <value>Tin Sheeting</value>
          </text>
          <text id="/data/walls:option5">
            <value>Other</value>
          </text>

