          </text>
          <text id="/data/roof:option0">
            <value>Tin Sheeting</value>
          </text>
          <text id="/data/roof:option1">
            <value>Cement</value>
          </text>
          <text id="/data/roof:option2">
            <value>Tiles</value>
          </text>
          <text id="/data/roof:option3">
            <value>Wood</value>
          </text>
          <text id="/data/roof:option4">
