          </text>
          <text id="/data/aircondition:option0">
            <value>No</value>
          </text>
          <text id="/data/aircondition:option1">
            <value>Some rooms airconditioned</value>
          </text>
          <text id="/data/aircondition:option2">
            <value>All rooms airconditioned</value>
          </text>

          <text id="/data/electricity:label">
