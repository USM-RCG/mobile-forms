          </text>
          <text id="/data/eaves:option0">
            <value>Open</value>
          </text>
          <text id="/data/eaves:option1">
            <value>Open but screened</value>
          </text>
          <text id="/data/eaves:option2">
            <value>Closed</value>
          </text>

          <text id="/data/patio:label">
            <value>Enter whether there is a patio and what its primary (most common) condition is</value>
          </text>
          <text id="/data/patio:option0">
            <value>No patio</value>
          </text>
          <text id="/data/patio:option1">
            <value>Unscreened patio</value>
          </text>
          <text id="/data/patio:option2">
            <value>Screened patio</value>
          </text>

          <text id="/data/doors:label">
            <value>Enter whether doors are screened</value>
          </text>
          <text id="/data/doors:option0">
            <value>No doors screened</value>
          </text>
          <text id="/data/doors:option1">
            <value>Some doors screened</value>
          </text>
          <text id="/data/doors:option2">
            <value>All doors screened</value>
          </text>

          <text id="/data/windows:label">
            <value>Enter whether windows are screened</value>
          </text>
          <text id="/data/windows:option0">
            <value>No windows screened</value>
          </text>
          <text id="/data/windows:option1">
            <value>Some windows screened</value>
          </text>
          <text id="/data/windows:option2">
            <value>All windows screened</value>
          </text>

          <text id="/data/aircondition:label">
