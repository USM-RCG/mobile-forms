<h:html xmlns="http://www.w3.org/2002/xforms" xmlns:h="http://www.w3.org/1999/xhtml" xmlns:ev="http://www.w3.org/2001/xml-events"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:jr="http://openrosa.org/javarosa">
    <h:head>
        <h:title>Testform</h:title>
        <model>
            <instance>
                <data id="testform">
        	    <processedByMirth />
                    <regionExtId />
                    <mapAreaExtId />
                    <sectorExtId />
                    <householdExtId />
                    <individualExtId />
		    <fieldworkerExtId />
		    <collectionDateTime />
                </data>
            </instance>
            <itext>
                <translation lang="eng">
                    <text id="/data/regionExtId:label">
                        <value>Region Ext Id</value>
                    </text>
                    <text id="/data/regionExtId:hint">
                        <value />
                    </text>
                    <text id="/data/mapAreaExtId:label">
                        <value>Map Area Ext Id</value>
                    </text>
                    <text id="/data/mapAreaExtId:hint">
                        <value />
                    </text>
                    <text id="/data/sectorExtId:label">
                        <value>Sector Ext Id</value>
                    </text>
                    <text id="/data/sectorExtId:hint">
                        <value></value>
                    </text>
                    <text id="/data/householdExtId:label">
                        <value>Household Ext Id</value>
                    </text>
                    <text id="/data/householdExtId:hint">
                        <value />
                    </text>
		    <text id="/data/individualExtId:label">
                        <value>Individual Ext Id</value>
                    </text>
                    <text id="/data/individualExtId:hint">
                        <value />
                    </text>
     		    <text id="/data/fieldworkerExtId:label">
                        <value>Field Worker Ext Id</value>
                    </text>
                    <text id="/data/fieldworkerExtId:hint">
                        <value />
                    </text>
		    <text id="/data/collectionDateTime:label">
                        <value>Collection Date and Time</value>
                    </text>
                    <text id="/data/collectionDateTime:hint">
                        <value />
                    </text>
                </translation>
            </itext>
            <bind nodeset="/data/processedByMirth" type="boolean" readonly="false()"/>
            <bind nodeset="/data/regionExtId" type="string" readonly="false()"/>
            <bind nodeset="/data/mapAreaExtId" type="string" readonly="false()"/>
            <bind nodeset="/data/sectorExtId" type="string" readonly="false()"/>
	    <bind nodeset="/data/householdExtId" type="string" readonly="false()"/>
	    <bind nodeset="/data/individualExtId" type="string" readonly="false()"/>
	    <bind nodeset="/data/fieldworkerExtId" type="string" readonly="false()"/>
            <bind nodeset="/data/collectionDateTime" type="string" readonly="false()" />
        </model>
    </h:head>
    <h:body>
        <input ref="/data/regionExtId">
            <label ref="jr:itext('/data/regionExtId:label')" />
            <hint ref="jr:itext('/data/regionExtId:hint')" />
        </input>
        <input ref="/data/mapAreaExtId">
            <label ref="jr:itext('/data/mapAreaExtId:label')" />
            <hint ref="jr:itext('/data/mapAreaExtId:hint')" />
        </input>
        <input ref="/data/sectorExtId">
            <label ref="jr:itext('/data/sectorExtId:label')" />
            <hint ref="jr:itext('/data/sectorExtId:hint')" />
        </input>
        <input ref="/data/householdExtId">
            <label ref="jr:itext('/data/householdExtId:label')" />
            <hint ref="jr:itext('/data/householdExtId:hint')" />
        </input>
	<input ref="/data/individualExtId">
            <label ref="jr:itext('/data/individualExtId:label')" />
            <hint ref="jr:itext('/data/individualExtId:hint')" />
        </input>
	<input ref="/data/fieldworkerExtId">
            <label ref="jr:itext('/data/fieldworkerExtId:label')" />
            <hint ref="jr:itext('/data/fieldworkerExtId:hint')" />
        </input>
	<input ref="/data/collectionDateTime">
            <label ref="jr:itext('/data/collectionDateTime:label')" />
            <hint ref="jr:itext('/data/collectionDateTime:hint')" />
        </input>
    </h:body>
</h:html>
