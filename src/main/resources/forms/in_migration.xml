<h:html xmlns="http://www.w3.org/2002/xforms"
        xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:jr="http://openrosa.org/javarosa">
    <h:head>
        <h:title>In Migration</h:title>
        <model>
            <instance>
                <data id="in_migration" version="12">

                    <openhdsTagName>giga-gegenschein</openhdsTagName>

                    <meta>
                        <instanceID/>
                    </meta>

                    <collectionDateTime/>
                    <processedByMirth/>
                    <needsReview/>
                    <entityUuid/>
                    <entityExtId/>
                    <fieldWorkerUuid/>
                    <fieldWorkerExtId/>

                    <visitExtId/>
                    <visitUuid/>
                    <locationExtId/>
                    <locationUuid/>
                    <individual>
                      <uuid/>
                      <extId/>
                    </individual>
                    <migrationType/>
                    <migrationDate/>
                    <migrationOrigin/>
                    <migrationReason/>
                </data>
            </instance>
            <itext>
                <translation lang="Spanish">
                    <text id="/data/individualExtId:label">
                        <value>ID Externo del Individuo</value>
                    </text>
                    <text id="/data/individualExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/fieldWorkerExtId:label">
                        <value>ID Externo del Encuestador</value>
                    </text>
                    <text id="/data/fieldWorkerExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/visitExtId:label">
                        <value>ID Externo de la Visit</value>
                    </text>
                    <text id="/data/visitExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/collectionDateTime:label">
                        <value>Fecha de colección</value>
                    </text>
                    <text id="/data/collectionDateTime:hint">
                        <value/>
                    </text>
                    <text id="/data/locationExtId:label">
                        <value>ID Externo de Locación</value>
                    </text>
                    <text id="/data/locationExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/individual:label">
                        <value>Por favor seleccione migrante</value>
                        <value form="buttonText">Buscar Migrante</value>
                        <value form="noAppErrorString">CIMS búsqueda individual no disponible</value>
                    </text>
                    <text id="/data/migrationType:label">
                        <value>Tipo de Migración</value>
                    </text>
                    <text id="/data/migrationType:hint">
                        <value/>
                    </text>
                    <text id="/data/individual/uuid:label">
                        <value>Identificación Migrante Interno</value>
                    </text>
                    <text id="/data/individual/extId:label">
                        <value>Identificación Migrante</value>
                    </text>

                    <text id="/data/migrationDate:label">
                        <value>Fecha de Migración</value>
                    </text>
                    <text id="/data/migrationDate:hint">
                        <value/>
                    </text>
                    <text id="/data/migrationOrigin:label">
                        <value>Origen de Migración</value>
                    </text>
                    <text id="/data/migrationOrigin:hint">
                        <value/>
                    </text>
                    <text id="/data/migrationReason:label">
                        <value>Razón de Migración</value>
                    </text>
                    <text id="/data/migrationReason:hint">
                        <value/>
                    </text>

                </translation>
                <translation lang="eng">
                    <text id="/data/collectionDateTime:label">
                        <value>Collection Date</value>
                    </text>
                    <text id="/data/collectionDateTime:hint">
                        <value/>
                    </text>
                    <text id="/data/fieldWorkerExtId:label">
                        <value>Field Worker Ext Id</value>
                    </text>
                    <text id="/data/fieldWorkerExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/visitExtId:label">
                        <value>Visit Ext Id</value>
                    </text>
                    <text id="/data/visitExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/locationExtId:label">
                        <value>Location Ext Id</value>
                    </text>
                    <text id="/data/locationExtId:hint">
                        <value/>
                    </text>
                    <text id="/data/individual:label">
                        <value>Please select migrant</value>
                        <value form="buttonText">Migrant Search</value>
                        <value form="noAppErrorString">CIMS individual search unavailable</value>
                    </text>
                    <text id="/data/individual/uuid:label">
                        <value>Internal Migrant ID</value>
                    </text>
                    <text id="/data/individual/extId:label">
                        <value>Migrant ID</value>
                    </text>
                    <text id="/data/migrationType:label">
                        <value>Migration Type</value>
                    </text>
                    <text id="/data/migrationType:hint">
                        <value/>
                    </text>
                    <text id="/data/migrationDate:label">
                        <value>Migration Date</value>
                    </text>
                    <text id="/data/migrationDate:hint">
                        <value/>
                    </text>
                    <text id="/data/migrationOrigin:label">
                        <value>Migration Origin</value>
                    </text>
                    <text id="/data/migrationOrigin:hint">
                        <value/>
                    </text>
                    <text id="/data/migrationReason:label">
                        <value>Migration Reason</value>
                    </text>
                    <text id="/data/migrationReason:hint">
                        <value/>
                    </text>
                </translation>
            </itext>
            <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
            <bind nodeset="/data/processedByMirth" type="boolean" readonly="true"/>
            <bind nodeset="/data/needsReview" type="string" readonly="true()"/>
            <bind nodeset="/data/collectionDateTime" type="datetime" readonly="true()"/>
            <bind nodeset="/data/fieldWorkerExtId" type="string" required="true()" readonly="true()"/>
            <bind nodeset="/data/visitExtId" type="string" required="true()" readonly="true()"/>
            <bind nodeset="/data/locationExtId" type="string" required="true()" readonly="true()"/>
            <bind nodeset="/data/individual/uuid" type="string" required="true()"/>
            <bind nodeset="/data/individual/extId" type="string" required="true()"/>
            <bind nodeset="/data/migrationType" type="string" required="true()" readonly="true()"/>
            <bind nodeset="/data/migrationDate" type="date" required="true()" constraint="not(. > today())"/>
            <bind nodeset="/data/migrationOrigin" type="string" required="true()"/>
            <bind nodeset="/data/migrationReason" type="string" required="true()"/>
        </model>
    </h:head>
    <h:body>
        <group ref="/data/individual" appearance="field-list" intent="org.cims_bioko.ENTITY_SEARCH(entity='INDIVIDUAL')">
            <label ref="jr:itext('/data/individual:label')"/>
            <input ref="uuid">
                <label ref="jr:itext('/data/individual/uuid:label')"/>
            </input>
            <input ref="extId">
                <label ref="jr:itext('/data/individual/extId:label')"/>
            </input>
        </group>
        <input ref="/data/migrationDate">
            <label ref="jr:itext('/data/migrationDate:label')"/>
            <hint ref="jr:itext('/data/migrationDate:hint')"/>
        </input>
        <group appearance="field-list">
            <input ref="/data/migrationOrigin">
                <label ref="jr:itext('/data/migrationOrigin:label')"/>
                <hint ref="jr:itext('/data/migrationOrigin:hint')"/>
            </input>
            <input ref="/data/migrationReason">
                <label ref="jr:itext('/data/migrationReason:label')"/>
                <hint ref="jr:itext('/data/migrationReason:hint')"/>
            </input>
        </group>
    </h:body>
</h:html>
