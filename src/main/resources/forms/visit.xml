<h:html xmlns="http://www.w3.org/2002/xforms"
        xmlns:h="http://www.w3.org/1999/xhtml"
        xmlns:ev="http://www.w3.org/2001/xml-events"
        xmlns:xsd="http://www.w3.org/2001/XMLSchema"
        xmlns:jr="http://openrosa.org/javarosa">
    <h:head>
        <h:title>Visit</h:title>
        <model>
            <instance>
                <data id="visit" version="${openhds.tag.number}">
                    <openhdsTagName>${openhds.tag.name}</openhdsTagName>

                    <processedByMirth />
                    <needsReview />
                    <visitExtId />
                    <fieldWorkerExtId />
                    <locationExtId />
                    <visitDate />
                </data>
            </instance>
            <itext>
                <translation lang="Spanish">
                    <text id="/data/visitExtId:label">
                        <value>ID Externo de la Visita</value>
                    </text>
                    <text id="/data/visitExtId:hint">
                        <value />
                    </text>
                    <text id="/data/fieldWorkerExtId:label">
                        <value>ID Externo del Encuestador</value>
                    </text>
                    <text id="/data/fieldWorkerExtId:hint">
                        <value />
                    </text>
                    <text id="/data/locationExtId:label">
                        <value>ID Externo de la Locaci√≥n</value>
                    </text>
                    <text id="/data/locationExtId:hint">
                        <value />
                    </text>
                    <text id="/data/visitDate:label">
                        <value>Fecha de la Visita</value>
                    </text>
                    <text id="/data/visitDate:hint">
                        <value />
                    </text>
                </translation>
                <translation lang="English">
                    <text id="/data/visitExtId:label">
                        <value>Visit Ext Id</value>
                    </text>
                    <text id="/data/visitExtId:hint">
                        <value />
                    </text>
                    <text id="/data/fieldWorkerExtId:label">
                        <value>Field Worker Ext Id</value>
                    </text>
                    <text id="/data/fieldWorkerExtId:hint">
                        <value />
                    </text>
                    <text id="/data/locationExtId:label">
                        <value>Location Ext Id</value>
                    </text>
                    <text id="/data/locationExtId:hint">
                        <value />
                    </text>
                    <text id="/data/visitDate:label">
                        <value>Visit Date</value>
                    </text>
                    <text id="/data/visitDate:hint">
                        <value />
                    </text>
                </translation>
            </itext>
            <bind nodeset="/data/versionId" type="string" readonly="true" />
            <bind nodeset="/data/processedByMirth" type="boolean" readonly="true" />
            <bind nodeset="/data/needsReview" type="string" readonly="true()" />
            <bind nodeset="/data/visitExtId" type="string" required="true()" readonly="true" />
            <bind nodeset="/data/fieldWorkerExtId" type="string" required="true()" readonly="true" />
            <bind nodeset="/data/locationExtId" type="string" required="true()" readonly="true" />
            <bind nodeset="/data/visitDate" type="date" required="true()" />
        </model>
    </h:head>
    <h:body>
        <group appearance="field-list">
            <input ref="/data/visitExtId">
                <label ref="jr:itext('/data/visitExtId:label')" />
                <hint ref="jr:itext('/data/visitExtId:hint')" />
            </input>
            <input ref="/data/fieldWorkerExtId">
                <label ref="jr:itext('/data/fieldWorkerExtId:label')" />
                <hint ref="jr:itext('/data/fieldWorkerExtId:hint')" />
            </input>
            <input ref="/data/locationExtId">
                <label ref="jr:itext('/data/locationExtId:label')" />
                <hint ref="jr:itext('/data/locationExtId:hint')" />
            </input>
            <input ref="/data/visitDate">
                <label ref="jr:itext('/data/visitDate:label')" />
                <hint ref="jr:itext('/data/visitDate:hint')" />
            </input>
        </group>
    </h:body>
</h:html>
