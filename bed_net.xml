      <bind nodeset="/data/meta/instanceID" type="string" readonly="true()" calculate="concat('uuid:', uuid())"/>
      <bind nodeset="/data/householdSize" type="string" readonly="true()"/>
      <bind nodeset="/data/fieldWorkerExtId" type="string" readonly="true()"/>
      <bind nodeset="/data/distributionDateTime" type="datetime" readonly="true()"/>
      <bind nodeset="/data/locationExtId" type="string" required="true()"/>
      <bind nodeset="/data/beds" type="int" required="true()"/>
      <bind nodeset="/data/netsCurrent" type="int" required="true()"/>
      <bind nodeset="/data/ITNsCurrent" type="int" required="true()" relevant="/data/netsCurrent &gt; 0"/>
      <bind nodeset="/data/ITNGoodN" type="int" required="true()" relevant="/data/ITNsCurrent &gt; 0" constraint="not(. &gt; (/data/ITNsCurrent))" jr:constraintMsg="jr:itext('/data/ITNGoodN:jr:constraintMsg')"/>
      <bind nodeset="/data/netsRecommended" type="int" readonly="true()" calculate="if((string(/data/ITNGoodN) = ''), /data/beds, (/data/beds - /data/ITNGoodN))"/>
      <bind nodeset="/data/netsSupplied" type="int" required="true()"/>
      <bind nodeset="/data/netCode" type="string" required="true()"/>
      <bind nodeset="/data/netsHung" type="int" required="true()"/>
      <bind nodeset="/data/walls" type="select1" required="true()"/>
